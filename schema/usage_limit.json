{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://lvlup.ai/schemas/usage_limit.json",
  "title": "UsageLimit",
  "type": "object",
  "properties": {
    "id": {"type": "integer"},
    "user_google_sub": {"type": "string"},
    "plan_tier": {"type": "string"},
    "period_start": {"type": "string", "format": "date-time"},
    "period_end": {"type": "string", "format": "date-time"},
    "total_input_tokens": {"type": "integer", "minimum": 0},
    "total_output_tokens": {"type": "integer", "minimum": 0},
    "total_requests": {"type": "integer", "minimum": 0},
    "created_at": {"type": "string", "format": "date-time"}
  },
  "required": ["user_google_sub", "plan_tier", "period_start", "period_end", "created_at"]
} 